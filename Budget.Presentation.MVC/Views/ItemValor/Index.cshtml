@model IEnumerable<Budget.Presentation.MVC.ViewModels.ItemValorViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create", new { id = ViewBag.OrcamentoId })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Vencimento)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Vencimento)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id }) |
            @Html.ActionLink("SubValores", "Index", "ItemSubValor", new { id = item.Id }, null)
        </td>
    </tr>
}

</table>

<div>
    @Html.ActionLink("Back to Orçamento", "Index", "Orcamento", new { id = ViewBag.OrcamentoId }, null)
</div>
